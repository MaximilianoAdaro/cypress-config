{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "sourceMap": false,
    "inlineSources": true,
    "noEmit": true,
    "allowJs": true,
    "types": ["cypress", "node", "@cypress/grep"],
    "target": "ES2019", // ES2019: https://github.com/webpack/webpack/issues/10227
    "lib": ["ES2020", "DOM"]
  },
  "include": ["src/**/*.ts"],
  "allowDeclarations": true
}
